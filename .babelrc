{
  "sourceMaps": "inline",
  "comments": false,
  "env": {
    "development": {
      "sourceMaps": "inline",
      "comments": false,
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "node": "current"
            }
          }
        ]
      ]
    },
    "umd": {
      "sourceMaps": "inline",
      "comments": false,
      "presets": [
        [
          "@babel/preset-env",
          {
            "modules": false,
            "targets": {
              "browsers": "last 2 versions"
            }
          }
        ]
      ]
    }
  }
}
